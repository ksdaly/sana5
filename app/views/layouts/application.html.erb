<!DOCTYPE html>
<html>
  <head>
    <title>SANA</title>
    <%= stylesheet_link_tag    "application", media: "all" %>
    <%= javascript_include_tag "application" %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    <%= render 'layouts/menu' %>

    <% flash.each do |name, msg| -%>
        <%= content_tag :div, msg, class: name %>
      <% end -%>
    <%= yield %>

    <script>
      new gnMenu( document.getElementById( 'gn-menu' ) );
    </script>

  </body>
</html>
