<ul>
  <strong>Your to-dos for the day:</strong>
  <% @user_to_dos.each do |user_to_do| %>
    <%= simple_form_for user_to_do, remote: true do |f| %>
      <%= f.check_box :completed, id: "user_to_do_completed#{user_to_do.id}"  %>
      <%= f.submit "Update" %>
      <%= f.label :completed, user_to_do.to_do.title %>
    <% end %>
  <% end %>
</ul>





